+	struct ast_sockaddr ouraddr;
+	int res = -1;
+
+	if (ast_strlen_zero(data)) {
+		ast_log(LOG_ERROR, "Cannot call %s without arguments\n", app_ciscopage);
+		return -1;
+	}
+
+	parse = ast_strdupa(data);
